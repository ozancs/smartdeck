{
  "meta": {
    "id": "google_chrome",
    "name": "Google Chrome",
    "author": "SmartDeck",
    "version": "1.0.0",
    "description": "Browser controls."
  },
  "buttons": [
    {
      "label": "New Tab",
      "type": "key",
      "combos": {
        "win": "CTRL+T",
        "mac": "GUI+T"
      },
      "icon": "online:mdi:tab-plus",
      "labelSize": 18,
      "labelColor": "#000000",
      "iconColor": "#ffffff",
      "btnBgColor": "#5B5B5B",
      "iconScale": 0
    },
    {
      "label": "Close Tab",
      "type": "key",
      "combos": {
        "win": "CTRL+W",
        "mac": "GUI+W"
      },
      "icon": "online:mdi:tab-remove",
      "labelSize": 18,
      "labelColor": "#000000",
      "iconColor": "#ffffff",
      "btnBgColor": "#5B5B5B",
      "iconScale": 0
    },
    {
      "label": "Reopen Tab",
      "type": "key",
      "combos": {
        "win": "CTRL+SHIFT+T",
        "mac": "GUI+SHIFT+T"
      },
      "icon": "online:mdi:undo",
      "labelSize": 18,
      "labelColor": "#000000",
      "iconColor": "#ffffff",
      "btnBgColor": "#5B5B5B",
      "iconScale": 0
    },
    {
      "label": "Next Tab",
      "type": "key",
      "combos": {
        "win": "CTRL+TAB",
        "mac": "CTRL+TAB"
